//BOOLEANS AND COMPARISON OPERATORS
scala> true
res21: Boolean = true

scala> false
res22: Boolean = false

scala> 1 > 2

res23: Boolean = false

scala> 2 < 1
res24: Boolean = false

scala> 3 >= 1
res27: Boolean = true

scala> 3 <= 3
res28: Boolean = true

scala> 2 == 1
res25: Boolean = false

scala> 2 != 1
res26: Boolean = true
